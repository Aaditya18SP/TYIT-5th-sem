<?xml version="1.0" encoding="utf-8" ?>
<Advertisements>
  <Ad> 
    <!--The image when the advertisement is displayed-->
    <!--Dont leave any space between the opening and closing tags-->
    <ImageUrl>https://img.freepik.com/free-photo/blue-black-muscle-car-with-license-plate-that-says-trans-front_1340-23399.jpg?w=2000</ImageUrl>

    <!--The URL to navigate to when clicked on it-->
    <NavigateUrl>https://www.google.com</NavigateUrl>

    <!--The text to display in the ad-->
    <AlternateText>Presenting the Ultimate Pagani Zonda!</AlternateText>

    <!--	The number indicating how often an advertisement will appear.-->
    <Impressions>1</Impressions>

    <!--Keyword identifying a group of advertisements. This is used for filtering.-->
    <Keyword>pagani_ads</Keyword>

    <!--The height of the ad-->
    <Height>150px</Height>

    <!--the width of the ad-->
    <Width>100%</Width>
    
  </Ad>

